<?xml version = "1.0" encoding = "utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.regres.application.ResourceTypes" table="resource_types">

		<meta attribute="class-description">
			This class contains the resource types.
		</meta>

		<id name="resourceTypeId" type="int" column="resource_type_id">
			<generator class="identity" />
		</id>

		<property name="typeName" column="type_name" type="string" />

	</class>
</hibernate-mapping>